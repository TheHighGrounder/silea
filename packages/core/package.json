		"@types/minimist": "1.2.0",
		"@types/node": "14.0.26",
		"@types/react": "16.9.43",
		"@types/react-dom": "16.9.8",
		"@types/react-helmet": "6.0.0",
		"@types/webpack": "4.41.21",
		"@types/webpack-bundle-analyzer": "3.8.0",
