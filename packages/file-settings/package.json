	"description": "A settings package for Silea that uses a silea.settings.js file.",
	"homepage": "https://github.com/sileajs/silea",
	"license": "SEE LICENSE IN LICENSE",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"keywords": [
		"silea",
		"silea-settings"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/sileajs/silea.git"
