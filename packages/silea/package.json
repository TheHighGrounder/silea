{
	"name": "silea",
	"version": "0.0.1",
	"description": "Silea cli and entry point to other packages",
	"keywords": [
		"silea",
		"cli"
	],
	"homepage": "https://sileajs.com",
	"license": "SEE LICENSE IN LICENSE",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"bin": {
		"silea": "dist/src/cli/index.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SileaJS/silea.git"
	},
	"bugs": {
		"url": "https://community.sileajs.com"
	},
	"scripts": {
		"start": "node dist/src/cli/index.js",
		"dev": "ts-node src/cli/index.ts",
		"ts": "ts-node",
		"test": "../../node_modules/.bin/jest --watch",
		"test:ci": "../../node_modules/.bin/jest --colors --ci --coverage",
		"test:inspect": "node --inspect ../../node_modules/jest/bin/jest.js --watch --no-cache --runInBand",
		"build": "tsc --project ./tsconfig.build.json && cp -a templates dist/templates",
		"build:watch": "tsc --project ./tsconfig.build.json --watch",
		"prepublish": "npm run build"
	},
	"peerDependencies": {
		"@sileajs/core": "0.0.1"
	},
	"dependencies": {
		"@emotion/core": "10.0.28",
		"@emotion/styled": "10.0.27",
		"@loadable/component": "5.13.1",
		"@sileajs/error": "0.0.1",
		"@sileajs/type-declarations": "0.0.1",
		"@sileajs/types": "0.0.1",
		"chalk": "4.1.0",
		"clipboardy": "2.3.0",
		"commander": "6.0.0",
		"detect-port-alt": "1.1.6",
		"didyoumean": "1.2.1",
		"envinfo": "7.7.2",
		"fs-extra": "9.0.1",
		"get-value": "3.0.1",
		"he": "1.2.0",
		"inquirer": "7.3.3",
		"is-root": "2.1.0",
		"node-fetch": "2.6.0",
		"ora": "4.0.5",
		"ramda": "0.27.0",
		"react-helmet-async": "1.0.6",
		"simple-entity-decode": "0.0.3",
		"tar": "6.0.2",
		"typed-emitter": "1.2.0",
		"typescript": "3.9.7"
	},
	"devDependencies": {
		"@sileajs/connect": "0.0.1",
		"@sileajs/core": "0.0.1",
		"@types/didyoumean": "1.2.0",
		"@types/fs-extra": "9.0.1",
		"@types/inquirer": "7.3.0",
		"@types/node": "14.0.26",
		"@types/node-fetch": "2.5.7",
		"@types/phin": "3.4.0",
		"@types/ramda": "0.27.11",
		"@types/shelljs": "0.8.8",
		"@types/stack-trace": "0.0.29",
		"@types/tar": "4.0.3",
		"lodash.omit": "4.5.0",
		"react-test-renderer": "16.13.1",
		"ts-node": "8.10.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
