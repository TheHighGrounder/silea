{
	"name": "example-mars-theme",
	"version": "0.0.1",
	"private": true,
	"description": "Example of a project using mars-theme.",
	"scripts": {
		"dev": "silea dev",
		"dev:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/silea dev",
		"build": "silea build",
		"build:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/silea build",
		"serve": "silea serve",
		"serve:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/silea serve",
		"now": "npx silea build && npx now --confirm --token $NOW_TOKEN",
		"now:alias": "npx now alias --token $NOW_TOKEN set mars-theme-example.now.sh mars-theme-$COMMIT_SHA.now.sh"
	},
	"license": "SEE LICENSE IN LICENSE",
	"engines": {
		"node": "12.x"
	},
	"dependencies": {
		"@sileajs/core": "^0.0.1",
		"@sileajs/html2react": "^0.0.1",
		"@sileajs/mars-theme": "^0.0.1",
		"@sileajs/tiny-router": "^0.0.1",
		"@sileajs/wp-source": "^0.0.1",
		"babel-plugin-silea": "^0.0.1",
		"silea": "^0.0.1"
	}
}
